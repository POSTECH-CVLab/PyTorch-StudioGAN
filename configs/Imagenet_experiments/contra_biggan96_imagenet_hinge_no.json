{
    "data_processing":{
      "dataset_name": "imagenet",
      "data_path": "./data/ILSVRC2012",
      "img_size": 96,
      "num_classes": 1000,
      "batch_size4prcsing": 256,
      "chunk_size": 500,
      "compression": false
    },
  
    "train": {
      "architecture": "biggan96",
      "print_every": 100,
      "save_every": 2000,
      "eval_dataset": false,
    
      "auxiliary_classifier": false,
      "projection_discriminator": false,
      "contrastive_training": true,
      "softmax_posterior": false,
      "contrastive_softmax": true,
      "hyper_dim":1024,
      "nonlinear_embed":false,
      "normalize_embed":true,
      "contrastive_lambda": 1.0,
      "tempering":true,
      "discrete_tempering":false,
      "tempering_times": "N/A",
      "start_temperature": 1.0,
      "end_temperature": 1.0,
      "g_spectral_norm": true,
      "d_spectral_norm": true,
      "leaky_relu": false,
      "attention": true,
      "at_after_th_gen_block": 4,
      "at_after_th_dis_block": 1,
      "g_init": "ortho",
      "d_init": "ortho",
      "gradient_penalty_for_dis": false,
      "lambda4lp": 0.0,
      "lambda4gp": 0.0,
      "weight_clipping_for_dis": false,
      "weight_clipping_bound": "N/A",
      "latent_op": false,
      "latent_op_rate":"N/A",
      "latent_op_step":"N/A",
      "latent_op_step4eval":"N/A",
      "latent_op_alpha":"N/A",
      "latent_op_beta":"N/A",
      "latent_norm_reg_weight":"N/A",
      "consistency_reg": false,
      "consistency_lambda":"N/A",
      "make_positive_aug":false, 
      "synchronized_bn": false,
      "ema": true,
    
      "ema_decay": 0.9999,
      "ema_start": 20000,
      "adv_loss": "hinge",
      "z_dim": 120,
      "shared_dim": 128,
      "g_conv_dim": 96,
      "d_conv_dim": 96,
      "ortho_reg": false,
      "lambda4ortho": "N/A",
    
      "batch_size": 256,
      "accumulation_steps": 8, 
      "total_step": 150000,
      "truncated_factor": 1,
      "prior": "gaussian",
      "g_steps_per_iter": 1,
      "d_steps_per_iter": 1,
      "d_lr": 0.0004,
      "g_lr": 0.0001,
      "beta1": 0.0,
      "beta2": 0.999,
      "batch4metrics": 50
      }
    }
  